{
  "name": "Simple Dark",
  "type": "dark",
  "author": "Hondry Travis",
  "maintainers": "travis<Hondry.Travis@outlook.com>",
  "semanticHighlighting": true,
  "semanticClass": "theme.simple-dark",
  "colors": {
    "activityBar.background": "#2c2f32",
    "activityBar.foreground": "#f0f0f0",
    "activityBarBadge.background": "#00bfff",
    "editor.background": "#24272a",
    "editor.foreground": "#dddddd",
    "editor.findMatchHighlightBackground": "#444444",
    "editor.hoverHighlightBackground": "#444444",
    "editor.selectionBackground": "#444444",
    "editor.selectionForeground": "#f0f0f0",
    "editor.selectionHighlightBackground": "#444444",
    "editor.wordHighlightBackground": "#444444",
    "editor.wordHighlightStrongBackground": "#444444",
    "editorWarning.foreground": "#f4433c",
    "editorInfo.foreground": "#00E673",
    "editorGroupHeader.tabsBackground": "#1d1e21",
    "tab.activeBackground": "#24272a",
    "tab.inactiveBackground": "#2c2f32",
    "gitDecoration.modifiedResourceForeground": "#8BE9FD",
    "gitDecoration.addedResourceForeground": "#00E673",
    "gitDecoration.deletedResourceForeground": "#FF6347",
    "sideBar.background": "#1d1e21",
    "sideBarTitle.foreground": "#f0f0f0",
    "list.errorForeground": "#FF6347",
    "list.warningForeground": "#FF6347",
    "minimap.findMatchHighlight": "#00E673",
    "panelTitle.activeBorder": "#00bfff",
    "statusBar.background": "#1d1e21",
    "statusBar.noFolderBackground": "#1d1e21",
    "terminal.background": "#24272a"
  },
  "tokenColors": [
    {
      "name": "Variables",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#dddddd"
      }
    },
    {
      "name": "Variables this ...",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "name": "Variables parameter ...",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "Variables constant ...",
      "scope": [
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#E1FFFF"
      }
    },
    {
      "name": "Variables less,sass ...",
      "scope": [
        "variable.other.interpolation"
      ],
      "settings": {
        "foreground": "#FF69B4"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#737373"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#f4433c"
      }
    },
    {
      "name": "Invalid HTML attribute name",
      "scope": [
        "invalid.deprecated.entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#00bfff"
      }
    },
    {
      "name": "Invalid Style attribute name",
      "scope": [
        "invalid.deprecated.media"
      ],
      "settings": {
        "foreground": "#00E673"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#FF69B4"
      }
    },
    {
      "name": "Keyword new",
      "scope": [
        "keyword.operator.new"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword new",
      "scope": [
        "keyword.type.cs"
      ],
      "settings": {
        "foreground": "#8BE9FD"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage"
      ],
      "settings": {
        "foreground": "#FF69B4"
      }
    },
    {
      "name": "Storage Type for c#",
      "scope": [
        "storage.type.cs"
      ],
      "settings": {
        "foreground": "#00bfff"
      }
    },
    {
      "name": "MarkDown - paragraph",
      "scope": [
        "meta.paragraph.markdown",
        "text.html.markdown"
      ],
      "settings": {
        "foreground": "#D2D3D9"
      }
    },
    {
      "name": "MarkDown - heading",
      "scope": [
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#FF69B4"
      }
    },
    {
      "name": "MarkDown - heading content",
      "scope": [
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#00bfff"
      }
    },
    {
      "name": "MarkDown - code mark",
      "scope": [
        "punctuation.definition.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "MarkDown - raw code mark",
      "scope": [
        "punctuation.definition.raw.markdown",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#00E673"
      }
    },
    {
      "name": "MarkDown - underline",
      "scope": [
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#00bfff"
      }
    },
    {
      "name": "MarkDown - inline underline",
      "scope": [
        "meta.link.inline.markdown"
      ],
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "MarkDown - bold",
      "scope": [
        "punctuation.definition.bold.markdown",
        "markup.bold.markdown"
      ],
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "MarkDown - list",
      "scope": [
        "punctuation.definition.list.begin.markdown",
        "markup.list.unnumbered.markdown"
      ],
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "MarkDown - image",
      "scope": [
        "punctuation.definition.string.begin.markdown"
      ],
      "settings": {
        "foreground": "#FF69B4"
      }
    },
    {
      "name": "MarkDown - image - link",
      "scope": [
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "#00bfff"
      }
    },
    {
      "name": "MarkDown - image - description",
      "scope": [
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#eefa7a"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "name": "Entity Attribute Name",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#00bfff"
      }
    },
    {
      "name": "Entity CSS: hover ...",
      "scope": [
        "entity.other.attribute-name.pseudo-element"
      ],
      "settings": {
        "foreground": "#FF69B4"
      }
    },
    {
      "name": "Entity TagName",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#FF69B4"
      }
    },
    {
      "name": "Native Object[Array,Object]",
      "scope": [
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#00bfff"
      }
    },
    {
      "name": "color of function name",
      "scope": [
        "meta.function.parameters",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#00E673"
      }
    },
    {
      "name": "Use for in HTML attr id",
      "scope": [
        "meta.toc-list.id.html",
        "meta.attribute-with-value.id"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Use for in HTML @",
      "scope": [
        "meta.tag.block.any"
      ],
      "settings": {
        "foreground": "#FF69B4"
      }
    },
    {
      "name": "String:quoted | template",
      "scope": [
        "string.quoted.single",
        "string.quoted.double",
        "string.quoted.triple",
        "string.quoted.other",
        "string.template"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Regular string",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#00E673"
      }
    },
    {
      "name": "Use for css",
      "scope": [
        "string.unquoted.attribute-value.css"
      ],
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.separator.continuation",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#dddddd"
      }
    },
    {
      "name": "Punctuation color at the start and end of string",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string.template.begin",
        "punctuation.definition.string.template.end"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "JSON color",
      "scope": [
        "string.json",
        "punctuation.support.type.property-name.begin",
        "punctuation.support.type.property-name.end",
        "variable.other.jsdoc"
      ],
      "settings": {
        "foreground": "#8BE9FD"
      }
    },
    {
      "name": "Escape character",
      "scope": [
        "punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "name": "Use for jsdoc comment",
      "scope": [
        "punctuation.definition.block.tag.jsdoc",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end"
      ],
      "settings": {
        "foreground": "#FF69B4"
      }
    },
    {
      "name": "Use for jsdoc comment",
      "scope": [
        "punctuation.definition.bracket.curly.begin.jsdoc",
        "punctuation.definition.bracket.curly.end.jsdoc"
      ],
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "less,css,sass,html",
      "scope": [
        "punctuation.separator.key-value",
        "punctuation.definition.variable",
        "punctuation.definition.keyword",
        "punctuation.definition.entity.css",
        "punctuation.definition.constant.css",
        "punctuation.definition.interpolation",
        "keyword.control.at-rule.import",
        "variable.other.less"
      ],
      "settings": {
        "foreground": "#FF69B4"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.class",
        "support.variable",
        "support.function",
        "support.type",
        "support.constant"
      ],
      "settings": {
        "foreground": "#8BE9FD"
      }
    }
  ]
}