{
  "name": "simple-dark",
  "displayName": "Simple Dark",
  "publisher": "travis",
  "description": "This one is Simple Dark Theme for VSCode",
  "version": "0.3.3",
  "icon": "logo.png",
  "keywords": [
    "vscode theme",
    "theme simple",
    "simple dark",
    "dark theme",
    "dark",
    "dark simple",
    "theme dark"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HondryTravis/simple-dark.git"
  },
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Themes"
  ],
  "scripts": {
    "start": "nodemon --watch './packages/**/*.js' --exec 'npm run build'",
    "build": "node build.js",
    "theme:build": "vsce package",
    "theme:publish": "vsce publish"
  },
  "contributes": {
    "themes": [
      {
        "label": "Simple Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/Simple Dark-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
