{
  "name": "simple-dark",
  "displayName": "Simple Dark",
  "publisher": "travis",
  "description": "Dark theme for VS Code",
  "version": "1.4.1",
  "icon": "logo.png",
  "keywords": [
    "vscode theme",
    "theme simple",
    "simple dark",
    "dark theme",
    "dark",
    "theme",
    "simple",
    "dark simple",
    "theme dark"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HondryTravis/simple-dark.git"
  },
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Themes"
  ],
  "scripts": {
    "start": "nodemon --watch './packages/**/*.js' --exec 'npm run build'",
    "build": "node build.js",
    "wcag": "node wcag-check.js",
    "deploy": "sh scripts/deploy.sh",
    "theme:build": "vsce package",
    "theme:publish": "vsce publish"
  },
  "contributes": {
    "themes": [
      {
        "label": "Simple Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark.json"
      },
      {
        "label": "Simple Dark Soft",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-soft.json"
      },
      {
        "label": "Simple Light",
        "uiTheme": "vs",
        "path": "./themes/light.json"
      },
      {
        "label": "Simple Light Soft",
        "uiTheme": "vs",
        "path": "./themes/light-soft.json"
      }
    ]
  },
  "devDependencies": {
    "color": "^4.0.0",
    "nodemon": "^2.0.6"
  }
}
